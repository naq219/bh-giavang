            <LinearLayout style="@style/layitem" >

                <TextView
                    android:id="@+id/num5"
                    style="@style/tvitem1"
                    android:layout_height="fill_parent"
                    android:text="5" />

                <TextView
                    android:id="@+id/rank5"
                    style="@style/tvitem2"
                    android:text="123" />
            </LinearLayout>

            <LinearLayout
