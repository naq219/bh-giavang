                style="@style/layitem" >

                <TextView
                    android:id="@+id/num2"
                    style="@style/tvitem3"
                    android:text="nhi" />

                <TextView
                    android:id="@+id/rank2"
                    style="@style/tvitem4"
                    android:text="123" />
            </LinearLayout>

            <LinearLayout style="@style/layitem" >

