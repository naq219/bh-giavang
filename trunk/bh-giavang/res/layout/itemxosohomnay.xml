                    style="@style/tvitem3"
                    android:text="Đặc biệt" />

                <TextView
                    android:id="@+id/rank0"
                    style="@style/tvitem4"
                    android:text="123" />
            </LinearLayout>

            <LinearLayout style="@style/layitem" >

                <TextView
                    android:id="@+id/num1"
                    style="@style/tvitem1"
                    android:layout_height="fill_parent"
                    android:text="nhat" />

                <TextView
                    android:id="@+id/rank1"
                    style="@style/tvitem2"
                    android:text="123" />
            </LinearLayout>

            <LinearLayout
