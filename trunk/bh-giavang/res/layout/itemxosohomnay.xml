                style="@style/layitem" >

                <TextView
                    android:id="@+id/num8"
                    style="@style/tvitem3"
                    android:text="8" />

                <TextView
                    android:id="@+id/rank8"
                    style="@style/tvitem4"
                    android:text="123" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <TextView
        style="@style/textWhite"
        android:layout_width="match_parent"
        android:layout_marginTop="5dp"
        android:background="@color/cam"
        android:gravity="center_horizontal"
        android:padding="4dp"
        android:text="Kết Quả Lô Tô" />

