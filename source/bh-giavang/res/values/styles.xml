<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="text" parent="android:Widget.Holo.Light.TextView">
        <item name="android:textColor">#000000</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="them_text" parent="text"></style>

    <style name="them_icon" parent="text"></style>

    <style name="them_arrow" parent="text"></style>

    <style name="line_setting" parent="text"></style>

    <style name="text_top" parent="android:Widget.Holo.Light.TextView">
        <item name="android:gravity">center</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="btn_top">
        <item name="android:layout_margin">7dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="rel_top">
        <item name="android:background">@drawable/bg_top</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/top_heigh</item>
    </style>

    <style name="btn_tab">
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/bg_tab_giavang</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="text_dialog" parent="@style/text">
        <item name="android:padding">5dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="spinnertv" parent="android:Widget.Holo.Light.TextView">
        <item name="android:background">@drawable/cbb</item>
       <item name="android:textColor">#324f85</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">wrap_content</item>
         <item name="android:gravity">center_vertical</item>
    </style>

</resources>